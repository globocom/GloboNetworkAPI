{
  "title": "Vip Put",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "vips"
  ],
  "properties": {
    "vips": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "service": {
            "type": "string"
          },
          "business": {
            "type": "string"
          },
          "environmentvip": {
            "type": "integer"
          },
          "ports": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": [
                    "integer",
                    "null"
                  ]
                },
                "port": {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 65535
                },
                "options": {
                  "type": "object",
                  "properties": {
                    "l4_protocol": {
                      "type": "integer"
                    },
                    "l7_protocol": {
                      "type": "integer"
                    }
                  }
                },
                "pools": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": [
                          "integer",
                          "null"
                        ]
                      },
                      "server_pool": {
                        "type": "integer"
                      },
                      "l7_rule": {
                        "type": "integer"
                      },
                      "l7_value": {
                        "type": [
                          "string",
                          "null"
                        ]
                      },
                      "order": {
                        "type": [
                          "integer",
                          "null"
                        ]
                      }
                    },
                    "required": [
                      "server_pool",
                      "l7_rule"
                    ],
                    "additionalProperties": false
                  }
                }
              }
            }
          },
          "groups_permissions": {
            "type": [
              "array",
              "null"
            ],
            "items": {
              "type": "object",
              "properties": {
                "user_group": {
                  "type": [
                    "integer",
                    "string"
                  ]
                },
                "read": {
                  "type": "boolean"
                },
                "write": {
                  "type": "boolean"
                },
                "delete": {
                  "type": "boolean"
                },
                "change_config": {
                  "type": "boolean"
                }
              },
              "required": [
                "user_group",
                "read",
                "write",
                "delete",
                "change_config"
              ]
            }
          },
          "users_permissions": {
            "type": [
              "array",
              "null"
            ],
            "items": {
              "type": "object",
              "properties": {
                "user": {
                  "type": [
                    "integer",
                    "string"
                  ]
                },
                "read": {
                  "type": "boolean"
                },
                "write": {
                  "type": "boolean"
                },
                "delete": {
                  "type": "boolean"
                },
                "change_config": {
                  "type": "boolean"
                }
              },
              "required": [
                "user",
                "read",
                "write",
                "delete",
                "change_config"
              ]
            }
          },
          "options": {
            "type": "object",
            "properties": {
              "timeout": {
                "type": "integer"
              },
              "cache_group": {
                "type": "integer"
              },
              "traffic_return": {
                "type": "integer"
              },
              "persistence": {
                "type": "integer"
              }
            }
          },
          "ipv6": {
            "type": [
              "integer",
              "null",
              "string"
            ],
            "pattern": "^auto$"
          },
          "ipv4": {
            "type": [
              "integer",
              "null",
              "string"
            ],
            "pattern": "^auto$"
          }
        }
      }
    }
  }
}
