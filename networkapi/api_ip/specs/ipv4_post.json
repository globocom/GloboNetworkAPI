{
  "title": "IPv4 Post",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "ips"
  ],
  "properties": {
    "networks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "oct1": {
            "type": "integer"
          },
          "oct2": {
            "type": "integer"
          },
          "oct3": {
            "type": "integer"
          },
          "oct4": {
            "type": "integer"
          },
          "networkipv4": {
            "type": "integer"
          },
          "reserve_all": {
            "type": "boolean"
          },
          "description": {
            "type": "string"
          },
          "equipments": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                }
              }
            }
          }
        },
        "required": [
          "networkipv4"
        ]
      }
    }
  }
}
