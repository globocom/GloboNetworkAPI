{
  "title": "Object Group Permission Post",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "ogps"
  ],
  "properties": {
    "ogps": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "null"
          },
          "user_group": {
            "type": "integer"
          },
          "object_type": {
            "type": "integer"
          },
          "object_value": {
            "type": "integer"
          },
          "read": {
            "type": "boolean"
          },
          "write": {
            "type": "boolean"
          },
          "change_config": {
            "type": "boolean"
          },
          "delete": {
            "type": "boolean"
          }
        },
        "required": [
          "user_group",
          "object_type",
          "object_value",
          "read",
          "write",
          "change_config",
          "delete"
        ]
      }
    }
  }
}
